## -*- mode: makefile; -*-

## Select c++11 mode
CXX_STD = CXX11

## Configure tells us about locations for
## ProtoBuf & GSL headers and library via the variables
PKG_CPPFLAGS = -I../src @PKG_CPPFLAGS@
PKG_LIBS = @PKG_LIBS@

SOURCES = $(wildcard base/*.cc indexers/*.cc parameter_updater/*.cc ranef_updater/*.cc prior_updater/*.cc util/*.cc)
OBJECTS = indexer_utils.o ranef_updaters.o training_data.pb.o $(SOURCES:.cc=.o)
